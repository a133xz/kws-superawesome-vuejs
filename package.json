{
  "author": "Antonio Rob",
  "license": "MIT",
  "name": "superawesome-vuejs",
  "version": "1.0.0",
  "description": "Simple SuperAwesome login + Vuejs",
  "main": "index.html",
  "keywords": [
    "superawesome",
    "parcel",
    "vue",
    "axios"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.22.10",
    "vue-template-compiler": "^2.6.10"
  },
  "scripts": {
    "start": "parcel src/index.html --https --no-cache",
    "build": "rm -rf ./dist && NODE_ENV=production parcel build src/index.html --no-source-maps",
    "deploy": "yarn build && firebase deploy"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {}
    }
  },
  "superawesome": {
    "baseURL": ""
  }
}
